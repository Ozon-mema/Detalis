DropdownButton.Parent = Frame
DropdownButton.BackgroundColor3 = Color3.fromRGB(100, 100, 255)
DropdownButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
DropdownButton.BorderSizePixel = 2
DropdownButton.Position = UDim2.new(0.113924049, 0, 0.8, 0)
DropdownButton.Size = UDim2.new(0, 121, 0, 20)
DropdownButton.Font = Enum.Font.SourceSans
DropdownButton.Text = "Список игроков"
DropdownButton.TextColor3 = Color3.fromRGB(255, 255, 255)
DropdownButton.TextSize = 14.000

DropdownFrame.Parent = Frame
DropdownFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
DropdownFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
DropdownFrame.BorderSizePixel = 2
DropdownFrame.Position = UDim2.new(0, 0, 1, 5)
DropdownFrame.Size = UDim2.new(0, 158, 0, 0)
DropdownFrame.Visible = false
DropdownFrame.ClipsDescendants = true

UIListLayout.Parent = DropdownFrame
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder

-- Scripts:

local function IIMAWH_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local RunService = game:GetService("RunService")
	local Players = game:GetService("Players")
	
	local toggleButton = script.Parent
	local textBox = script.Parent.Parent:FindFirstChild("TextBox")
	local dropdownButton = script.Parent.Parent:FindFirstChild("DropdownButton")
	local dropdownFrame = script.Parent.Parent:FindFirstChild("DropdownFrame")
	local isFlinging = false
	local connection = nil
	local fallProtectionConnection = nil
	local originalPosition = nil
	local playerButtons = {}
	
	-- События для отслеживания игроков
	local playerAddedConnection = nil
	local playerRemovingConnection = nil
	
	if not ReplicatedStorage:FindFirstChild("juisdfj0i32i0eidsuf0iok") then
		local detection = Instance.new("Decal")
		detection.Name = "juisdfj0i32i0eidsuf0iok"
		detection.Parent = ReplicatedStorage
	end
	
	-- Функция для обновления списка игроков
	local function updatePlayersList()
		-- Очищаем старые кнопки
		for _, button in pairs(playerButtons) do
			button:Destroy()
		end
		playerButtons = {}
		
		-- Создаем кнопки для каждого игрока
		local players = Players:GetPlayers()
		table.sort(players, function(a, b)
			return a.Name:lower() < b.Name:lower()
		end)
		
		local height = 0
		for _, player in pairs(players) do
			if player ~= Players.LocalPlayer then
				local playerButton = Instance.new("TextButton")
				playerButton.Name = player.Name
				playerButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
				playerButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
				playerButton.BorderSizePixel = 1
				playerButton.Size = UDim2.new(1, 0, 0, 25)
				playerButton.Font = Enum.Font.SourceSans
				playerButton.Text = player.Name
				playerButton.TextColor3 = Color3.fromRGB(255, 255, 255)
				playerButton.TextSize = 14.000
				playerButton.Parent = dropdownFrame
				
				playerButton.MouseButton1Click:Connect(function()
					textBox.Text = player.Name
					dropdownFrame.Visible = false
					dropdownFrame.Size = UDim2.new(0, 158, 0, 0)
				end)
				
				table.insert(playerButtons, playerButton)
				height = height + 25
			end
		end
		
		-- Обновляем размер dropdown
		if dropdownFrame.Visible then
			dropdownFrame.Size = UDim2.new(0, 158, 0, math.min(height, 150))
		end
	end
	
	-- Функция для переключения видимости списка игроков
	local function toggleDropdown()
		dropdownFrame.Visible = not dropdownFrame.Visible
		if dropdownFrame.Visible then
			updatePlayersList()
		else
			dropdownFrame.Size = UDim2.new(0, 158, 0, 0)
		end
	end
	
	-- Инициализация списка игроков
	updatePlayersList()
	
	-- Подписываемся на события игроков
	playerAddedConnection = Players.PlayerAdded:Connect(function(player)
		updatePlayersList()
	end)
	
	playerRemovingConnection = Players.PlayerRemoving:Connect(function(player)
		updatePlayersList()
		-- Если целевой игрок вышел, останавливаем флинг
		if isFlinging and textBox.Text == player.Name then
			stopFlinging()
			textBox.Text = ""
			textBox.PlaceholderText = "Игрок вышел из игры"
		end
	end)
	
	local function checkBoundary(position)
		local boundary = 2000
		return math.abs(position.X) > boundary or math.abs(position.Y) > boundary or math.abs(position.Z) > boundary
	end
	
	-- Функция для проверки падения под карту
	local function checkFallUnderMap(position)
		local deathHeight = -100 -- Высота, при достижении которой считается, что игрок упал под карту
		return position.Y < deathHeight
	end
	
	-- Функция для защиты от падения
	local function setupFallProtection()
		if fallProtectionConnection then
			fallProtectionConnection:Disconnect()
			fallProtectionConnection = nil
		end
		
		fallProtectionConnection = RunService.Heartbeat:Connect(function()
			local lp = Players.LocalPlayer
			local char = lp.Character
			local hrp = char and char:FindFirstChild("HumanoidRootPart")
			
			if hrp and originalPosition then
				-- Проверяем, не упал ли игрок под карту
				if checkFallUnderMap(hrp.Position) then
					-- Мгновенно возвращаем на исходную позицию
					hrp.CFrame = originalPosition
					hrp.Velocity = Vector3.new(0, 0, 0)
				end
			end
		end)
	end
	
	local function stopFlinging()
		isFlinging = false
		toggleButton.Text = "FLING"
		toggleButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		
		if connection then
			connection:Disconnect()
			connection = nil
		end
		
		if fallProtectionConnection then
			fallProtectionConnection:Disconnect()
			fallProtectionConnection = nil
		end
	end
	
	local function flingTarget(targetPlayer)
		local lp = Players.LocalPlayer
		local targetChar = targetPlayer.Character
		local targetHRP = targetChar and targetChar:FindFirstChild("HumanoidRootPart")
		
		if not targetHRP then 
			textBox.Text = ""
			textBox.PlaceholderText = "У игрока нет HRP!"
			stopFlinging()
			return 
		end
		
		-- Сохраняем исходную позицию
		local lpChar = lp.Character
		local lpHRP = lpChar and lpChar:FindFirstChild("HumanoidRootPart")
		if lpHRP then
			originalPosition = lpHRP.CFrame
			-- Активируем защиту от падения
			setupFallProtection()
		else
			textBox.Text = ""
			textBox.PlaceholderText = "Нет HRP у вас!"
			stopFlinging()
			return
		end
		
		local movel = 0.1
		
		-- Отключаем предыдущее соединение если оно есть
		if connection then
			connection:Disconnect()
			connection = nil
		end
		
		connection = RunService.Heartbeat:Connect(function()
			local c = lp.Character
			local hrp = c and c:FindFirstChild("HumanoidRootPart")
			local targetChar = targetPlayer.Character
			local targetHRP = targetChar and targetChar:FindFirstChild("HumanoidRootPart")
			
			if not hrp or not targetHRP or not isFlinging then
				stopFlinging()
				return
			end
			
			-- Проверяем, существует ли игрок еще
			if not Players:FindFirstChild(targetPlayer.Name) then
				textBox.Text = ""
				textBox.PlaceholderText = "Игрок вышел из игры"
				stopFlinging()
				return
			end
			
			-- Телепортируемся к цели
			hrp.CFrame = targetHRP.CFrame
			
			-- Проверяем, не улетел ли игрок за карту
			if checkBoundary(hrp.Position) then
				-- Мгновенно возвращаем на исходную позицию
				if originalPosition then
					hrp.CFrame = originalPosition
				end
				return
			end
			
			-- Применяем флинг
			local vel = hrp.Velocity
			hrp.Velocity = vel * 10000 + Vector3.new(0, 10000, 0)
			RunService.RenderStepped:Wait()
			hrp.Velocity = vel
			RunService.Stepped:Wait()
			hrp.Velocity = vel + Vector3.new(0, movel, 0)
			movel = -movel
		end)
	end
	
	toggleButton.MouseButton1Click:Connect(function()
		if isFlinging then
			stopFlinging()
			return
		end
		
		if textBox.Text == "" then
			textBox.PlaceholderText = "Введите никнейм сначала!"
			return
		end
		
		local targetPlayer = Players:FindFirstChild(textBox.Text)
		if not targetPlayer then
			textBox.Text = ""
			textBox.PlaceholderText = "Игрок не найден!"
			return
		end
		
		if targetPlayer == Players.LocalPlayer then
			textBox.Text = ""
			textBox.PlaceholderText = "Нельзя флингать себя!"
			return
		end
		
		isFlinging = true
		toggleButton.Text = "STOP"
		toggleButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
		
		flingTarget(targetPlayer)
	end)
	
	-- Кнопка выпадающего списка
	dropdownButton.MouseButton1Click:Connect(function()
		toggleDropdown()
	end)
	
	-- Закрытие dropdown при клике вне его
	local function onInputBegan(input, gameProcessed)
		if gameProcessed then return end
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			local mousePos = game:GetService("UserInputService"):GetMouseLocation()
			local dropdownAbsolutePos = dropdownFrame.AbsolutePosition
			local dropdownAbsoluteSize = dropdownFrame.AbsoluteSize
			
			if dropdownFrame.Visible then
				if not (mousePos.X >= dropdownAbsolutePos.X and mousePos.X <= dropdownAbsolutePos.X + dropdownAbsoluteSize.X and
					   mousePos.Y >= dropdownAbsolutePos.Y and mousePos.Y <= dropdownAbsolutePos.Y + dropdownAbsoluteSize.Y) then
					dropdownFrame.Visible = false
					dropdownFrame.Size = UDim2.new(0, 158, 0, 0)
				end
			end
		end
	end
	
	game:GetService("UserInputService").InputBegan:Connect(onInputBegan)
	
	-- Автоматическая остановка при выходе из игры
	game:GetService("Players").LocalPlayer.CharacterRemoving:Connect(function()
		stopFlinging()
	end)
	
	-- Очистка соединений при уничтожении GUI
	Frame.Destroying:Connect(function()
		if playerAddedConnection then
			playerAddedConnection:Disconnect()
		end
		if playerRemovingConnection then
			playerRemovingConnection:Disconnect()
		end
	end)
	
end
coroutine.wrap(IIMAWH_fake_script)()
local function QCJQJL_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(QCJQJL_fake_script)()
