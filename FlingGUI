-- Touch fling gui by nickname

-- Gui to Lua (VIP VERSION)
-- Version: 6.9

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local Frame_2 = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local TextBox = Instance.new("TextBox")
local TextButton = Instance.new("TextButton")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false
print("Loaded!")

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.388539821, 0, 0.427821517, 0)
Frame.Size = UDim2.new(0, 158, 0, 140)

Frame_2.Parent = Frame
Frame_2.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Frame_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame_2.BorderSizePixel = 0
Frame_2.Size = UDim2.new(0, 158, 0, 25)

TextLabel.Parent = Frame_2
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.BorderSizePixel = 0
TextLabel.Position = UDim2.new(0.112792775, 0, -0.0151660154, 0)
TextLabel.Size = UDim2.new(0, 121, 0, 26)
TextLabel.Font = Enum.Font.Sarpanch
TextLabel.Text = "Touch Fling"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextSize = 25.000

TextBox.Parent = Frame
TextBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextBox.BorderSizePixel = 2
TextBox.Position = UDim2.new(0.113924049, 0, 0.25, 0)
TextBox.Size = UDim2.new(0, 121, 0, 25)
TextBox.Font = Enum.Font.SourceSans
TextBox.PlaceholderText = "Введите никнейм"
TextBox.Text = ""
TextBox.TextColor3 = Color3.fromRGB(0, 0, 0)
TextBox.TextSize = 14.000

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextButton.BorderColor3 = Color3.fromRGB(255, 255, 255)
TextButton.BorderSizePixel = 0
TextButton.Position = UDim2.new(0.113924049, 0, 0.55, 0)
TextButton.Size = UDim2.new(0, 121, 0, 37)
TextButton.Font = Enum.Font.SourceSansItalic
TextButton.Text = "FLING"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextSize = 20.000

-- Scripts:

local function IIMAWH_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local RunService = game:GetService("RunService")
	local Players = game:GetService("Players")
	
	local toggleButton = script.Parent
	local textBox = script.Parent.Parent:FindFirstChild("TextBox")
	local isFlinging = false
	local connection = nil
	local fallProtectionConnection = nil
	local originalPosition = nil
	
	if not ReplicatedStorage:FindFirstChild("juisdfj0i32i0eidsuf0iok") then
		local detection = Instance.new("Decal")
		detection.Name = "juisdfj0i32i0eidsuf0iok"
		detection.Parent = ReplicatedStorage
	end
	
	local function checkBoundary(position)
		local boundary = 2000
		return math.abs(position.X) > boundary or math.abs(position.Y) > boundary or math.abs(position.Z) > boundary
	end
	
	-- Функция для проверки падения под карту
	local function checkFallUnderMap(position)
		local deathHeight = -100 -- Высота, при достижении которой считается, что игрок упал под карту
		return position.Y < deathHeight
	end
	
	-- Функция для защиты от падения
	local function setupFallProtection()
		if fallProtectionConnection then
			fallProtectionConnection:Disconnect()
			fallProtectionConnection = nil
		end
		
		fallProtectionConnection = RunService.Heartbeat:Connect(function()
			local lp = Players.LocalPlayer
			local char = lp.Character
			local hrp = char and char:FindFirstChild("HumanoidRootPart")
			
			if hrp and originalPosition then
				-- Проверяем, не упал ли игрок под карту
				if checkFallUnderMap(hrp.Position) then
					-- Мгновенно возвращаем на исходную позицию
					hrp.CFrame = originalPosition
					hrp.Velocity = Vector3.new(0, 0, 0)
				end
			end
		end)
	end
	
	local function stopFlinging()
		isFlinging = false
		toggleButton.Text = "FLING"
		toggleButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		
		if connection then
			connection:Disconnect()
			connection = nil
		end
		
		if fallProtectionConnection then
			fallProtectionConnection:Disconnect()
			fallProtectionConnection = nil
		end
	end
	
	local function flingTarget(targetPlayer)
		local lp = Players.LocalPlayer
		local targetChar = targetPlayer.Character
		local targetHRP = targetChar and targetChar:FindFirstChild("HumanoidRootPart")
		
		if not targetHRP then 
			textBox.Text = ""
			textBox.PlaceholderText = "У игрока нет HRP!"
			stopFlinging()
			return 
		end
		
		-- Сохраняем исходную позицию
		local lpChar = lp.Character
		local lpHRP = lpChar and lpChar:FindFirstChild("HumanoidRootPart")
		if lpHRP then
			originalPosition = lpHRP.CFrame
			-- Активируем защиту от падения
			setupFallProtection()
		else
			textBox.Text = ""
			textBox.PlaceholderText = "Нет HRP у вас!"
			stopFlinging()
			return
		end
		
		local movel = 0.1
		
		-- Отключаем предыдущее соединение если оно есть
		if connection then
			connection:Disconnect()
			connection = nil
		end
		
		connection = RunService.Heartbeat:Connect(function()
			local c = lp.Character
			local hrp = c and c:FindFirstChild("HumanoidRootPart")
			local targetChar = targetPlayer.Character
			local targetHRP = targetChar and targetChar:FindFirstChild("HumanoidRootPart")
			
			if not hrp or not targetHRP or not isFlinging then
				stopFlinging()
				return
			end
			
			-- Телепортируемся к цели
			hrp.CFrame = targetHRP.CFrame
			
			-- Проверяем, не улетел ли игрок за карту
			if checkBoundary(hrp.Position) then
				-- Мгновенно возвращаем на исходную позицию
				if originalPosition then
					hrp.CFrame = originalPosition
				end
				return
			end
			
			-- Применяем флинг
			local vel = hrp.Velocity
			hrp.Velocity = vel * 10000 + Vector3.new(0, 10000, 0)
			RunService.RenderStepped:Wait()
			hrp.Velocity = vel
			RunService.Stepped:Wait()
			hrp.Velocity = vel + Vector3.new(0, movel, 0)
			movel = -movel
		end)
	end
	
	toggleButton.MouseButton1Click:Connect(function()
		if isFlinging then
			stopFlinging()
			return
		end
		
		if textBox.Text == "" then
			textBox.PlaceholderText = "Введите никнейм сначала!"
			return
		end
		
		local targetPlayer = Players:FindFirstChild(textBox.Text)
		if not targetPlayer then
			textBox.Text = ""
			textBox.PlaceholderText = "Игрок не найден!"
			return
		end
		
		if targetPlayer == Players.LocalPlayer then
			textBox.Text = ""
			textBox.PlaceholderText = "Нельзя флингать себя!"
			return
		end
		
		isFlinging = true
		toggleButton.Text = "STOP"
		toggleButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
		
		flingTarget(targetPlayer)
	end)
	
	-- Автоматическая остановка при выходе из игры
	game:GetService("Players").LocalPlayer.CharacterRemoving:Connect(function()
		stopFlinging()
	end)
	
end
coroutine.wrap(IIMAWH_fake_script)()
local function QCJQJL_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(QCJQJL_fake_script)()
