-- Advanced Touch Fling by Nickname with Auto-Complete

-- Instances:
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local Frame_2 = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local TargetLabel = Instance.new("TextLabel")
local TextBox = Instance.new("TextBox")
local TextButton = Instance.new("TextButton")
local StatusLabel = Instance.new("TextLabel")

-- Properties:
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.388539821, 0, 0.427821517, 0)
Frame.Size = UDim2.new(0, 220, 0, 210)

Frame_2.Parent = Frame
Frame_2.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Frame_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame_2.BorderSizePixel = 0
Frame_2.Size = UDim2.new(0, 220, 0, 25)

TextLabel.Parent = Frame_2
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.BorderSizePixel = 0
TextLabel.Size = UDim2.new(0, 220, 0, 25)
TextLabel.Font = Enum.Font.Sarpanch
TextLabel.Text = "Advanced Fling"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextSize = 20.000

TargetLabel.Parent = Frame
TargetLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TargetLabel.BackgroundTransparency = 1.000
TargetLabel.Position = UDim2.new(0.05, 0, 0.15, 0)
TargetLabel.Size = UDim2.new(0, 200, 0, 20)
TargetLabel.Font = Enum.Font.SourceSans
TargetLabel.Text = "–¶–µ–ª—å: –ù–µ –≤—ã–±—Ä–∞–Ω–∞"
TargetLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TargetLabel.TextSize = 14.000
TargetLabel.TextXAlignment = Enum.TextXAlignment.Left

TextBox.Parent = Frame
TextBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextBox.BorderSizePixel = 2
TextBox.Position = UDim2.new(0.05, 0, 0.3, 0)
TextBox.Size = UDim2.new(0, 200, 0, 25)
TextBox.Font = Enum.Font.SourceSans
TextBox.PlaceholderText = "–í–≤–µ–¥–∏—Ç–µ —á–∞—Å—Ç—å –Ω–∏–∫–∞"
TextBox.Text = ""
TextBox.TextColor3 = Color3.fromRGB(0, 0, 0)
TextBox.TextSize = 14.000

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextButton.BorderColor3 = Color3.fromRGB(255, 255, 255)
TextButton.BorderSizePixel = 0
TextButton.Position = UDim2.new(0.05, 0, 0.55, 0)
TextButton.Size = UDim2.new(0, 200, 0, 30)
TextButton.Font = Enum.Font.SourceSansItalic
TextButton.Text = "START FLING"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextSize = 16.000

StatusLabel.Parent = Frame
StatusLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
StatusLabel.BackgroundTransparency = 1.000
StatusLabel.Position = UDim2.new(0.05, 0, 0.75, 0)
StatusLabel.Size = UDim2.new(0, 200, 0, 20)
StatusLabel.Font = Enum.Font.SourceSans
StatusLabel.Text = "–°—Ç–∞—Ç—É—Å: –û–∂–∏–¥–∞–Ω–∏–µ"
StatusLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
StatusLabel.TextSize = 14.000

-- Scripts:
local function MainScript()
	local script = TextButton:FindFirstChild("LocalScript") or Instance.new('LocalScript', TextButton)
	
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local RunService = game:GetService("RunService")
	local Players = game:GetService("Players")
	local Workspace = game:GetService("Workspace")
	
	local localPlayer = Players.LocalPlayer
	local toggleButton = script.Parent
	local textBox = Frame:FindFirstChild("TextBox")
	local statusLabel = Frame:FindFirstChild("StatusLabel")
	local targetLabel = Frame:FindFirstChild("TargetLabel")
	
	local isFlinging = false
	local originalCFrame = nil
	local originalCameraCFrame = nil
	local flingConnection = nil
	local cameraConnection = nil
	local monitorConnection = nil
	local currentTargetPlayer = nil
	
	-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–∞ –ø–æ —á–∞—Å—Ç–∏ –Ω–∏–∫–Ω–µ–π–º–∞
	local function findPlayerByPartialName(partialName)
		if partialName == "" then
			return nil
		end
		
		local lowerPartial = string.lower(partialName)
		local foundPlayers = {}
		
		-- –ò—â–µ–º –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤, —á–µ–π –Ω–∏–∫–Ω–µ–π–º —Å–æ–¥–µ—Ä–∂–∏—Ç –≤–≤–µ–¥–µ–Ω–Ω—É—é —á–∞—Å—Ç—å
		for _, player in ipairs(Players:GetPlayers()) do
			if player ~= localPlayer then -- –ò—Å–∫–ª—é—á–∞–µ–º —Å–∞–º–æ–≥–æ —Å–µ–±—è
				local playerName = string.lower(player.Name)
				if string.find(playerName, lowerPartial, 1, true) then
					table.insert(foundPlayers, player)
				end
			end
		end
		
		-- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –ø–µ—Ä–≤–æ–≥–æ –Ω–∞–π–¥–µ–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞
		if #foundPlayers > 0 then
			return foundPlayers[1]
		end
		
		return nil
	end
	
	-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ —Ü–µ–ª–∏
	local function updateTargetInfo(player)
		if player then
			targetLabel.Text = "–¶–µ–ª—å: " .. player.Name
			targetLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
		else
			targetLabel.Text = "–¶–µ–ª—å: –ù–µ –≤—ã–±—Ä–∞–Ω–∞"
			targetLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
		end
		currentTargetPlayer = player
	end
	
	-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ç–µ–∫—Å—Ç–∞ –≤ TextBox
	textBox:GetPropertyChangedSignal("Text"):Connect(function()
		local text = textBox.Text
		if text == "" then
			updateTargetInfo(nil)
			return
		end
		
		local foundPlayer = findPlayerByPartialName(text)
		updateTargetInfo(foundPlayer)
		
		-- –ï—Å–ª–∏ –∏–≥—Ä–æ–∫ –Ω–µ –Ω–∞–π–¥–µ–Ω, –ø–æ–∫–∞–∑—ã–≤–∞–µ–º —Å–æ–æ–±—â–µ–Ω–∏–µ
		if not foundPlayer then
			targetLabel.Text = "–¶–µ–ª—å: –ò–≥—Ä–æ–∫ –Ω–µ –Ω–∞–π–¥–µ–Ω"
			targetLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
		end
	end)
	
	-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏, —É–ª–µ—Ç–µ–ª –ª–∏ –∏–≥—Ä–æ–∫ –∑–∞ –∫–∞—Ä—Ç—É
	local function isPlayerOutOfBounds(player)
		local character = player.Character
		if not character then return true end
		
		local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
		if not humanoidRootPart then return true end
		
		local position = humanoidRootPart.Position
		local mapBoundary = 5000 -- –ì—Ä–∞–Ω–∏—Ü–∞ –∫–∞—Ä—Ç—ã
		
		-- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –Ω–µ –≤—ã—à–µ–ª –ª–∏ –∏–≥—Ä–æ–∫ –∑–∞ –ø—Ä–µ–¥–µ–ª—ã –∫–∞—Ä—Ç—ã
		if math.abs(position.X) > mapBoundary or 
		   math.abs(position.Y) > mapBoundary or 
		   math.abs(position.Z) > mapBoundary then
			return true
		end
		
		-- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –Ω–µ —É–ø–∞–ª –ª–∏ –∏–≥—Ä–æ–∫ –ø–æ–¥ –∫–∞—Ä—Ç—É
		if position.Y < -500 then
			return true
		end
		
		return false
	end
	
	-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω–æ–π –ø–æ–∑–∏—Ü–∏–∏ –∏ –∫–∞–º–µ—Ä—ã
	local function saveOriginalState()
		local character = localPlayer.Character
		if character then
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if humanoidRootPart then
				originalCFrame = humanoidRootPart.CFrame
			end
		end
		
		local camera = Workspace.CurrentCamera
		if camera then
			originalCameraCFrame = camera.CFrame
		end
	end
	
	-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω–æ–≥–æ —Å–æ—Å—Ç–æ—è–Ω–∏—è
	local function restoreOriginalState()
		isFlinging = false
		
		-- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –ø–æ–∑–∏—Ü–∏—é –∏–≥—Ä–æ–∫–∞
		local character = localPlayer.Character
		if character and originalCFrame then
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if humanoidRootPart then
				humanoidRootPart.CFrame = originalCFrame
			end
		end
		
		-- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∫–∞–º–µ—Ä—É
		local camera = Workspace.CurrentCamera
		if camera and originalCameraCFrame then
			camera.CFrame = originalCameraCFrame
			camera.CameraType = Enum.CameraType.Custom
		end
		
		-- –û—Ç–∫–ª—é—á–∞–µ–º –≤—Å–µ —Å–æ–µ–¥–∏–Ω–µ–Ω–∏—è
		if flingConnection then
			flingConnection:Disconnect()
			flingConnection = nil
		end
		
		if cameraConnection then
			cameraConnection:Disconnect()
			cameraConnection = nil
		end
		
		if monitorConnection then
			monitorConnection:Disconnect()
			monitorConnection = nil
		end
		
		-- –û–±–Ω–æ–≤–ª—è–µ–º UI
		toggleButton.Text = "START FLING"
		toggleButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		statusLabel.Text = "–°—Ç–∞—Ç—É—Å: –û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ"
	end
	
	-- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Ñ–ª–∏–Ω–≥–∞
	local function startFling(targetPlayer)
		if not targetPlayer or not targetPlayer.Character then
			statusLabel.Text = "–û—à–∏–±–∫–∞: –ò–≥—Ä–æ–∫ –Ω–µ –Ω–∞–π–¥–µ–Ω"
			return
		end
		
		-- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –Ω–µ –ø—ã—Ç–∞–µ—Ç—Å—è –ª–∏ –∏–≥—Ä–æ–∫ —Ñ–ª–∏–Ω–≥–∞—Ç—å —Å–∞–º–æ–≥–æ —Å–µ–±—è
		if targetPlayer == localPlayer then
			statusLabel.Text = "–û—à–∏–±–∫–∞: –ù–µ–ª—å–∑—è —Ñ–ª–∏–Ω–≥–∞—Ç—å —Å–µ–±—è!"
			targetLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
			return
		end
		
		saveOriginalState()
		isFlinging = true
		statusLabel.Text = "–°—Ç–∞—Ç—É—Å: –§–ª–∏–Ω–≥ –∞–∫—Ç–∏–≤–µ–Ω"
		
		local targetCharacter = targetPlayer.Character
		local targetHRP = targetCharacter:FindFirstChild("HumanoidRootPart")
		local localCharacter = localPlayer.Character
		local localHRP = localCharacter and localCharacter:FindFirstChild("HumanoidRootPart")
		
		if not targetHRP or not localHRP then
			restoreOriginalState()
			return
		end
		
		-- –ù–∞—Å—Ç—Ä–∞–∏–≤–∞–µ–º –∫–∞–º–µ—Ä—É –Ω–∞ —Ü–µ–ª—å
		local camera = Workspace.CurrentCamera
		if camera then
			camera.CameraType = Enum.CameraType.Scriptable
			cameraConnection = RunService.RenderStepped:Connect(function()
				if isFlinging and targetHRP then
					camera.CFrame = CFrame.new(targetHRP.Position + Vector3.new(0, 5, 10), targetHRP.Position)
				end
			end)
		end
		
		-- –ú–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥ –ø–æ–ª–æ–∂–µ–Ω–∏—è —Ü–µ–ª–∏
		monitorConnection = RunService.Heartbeat:Connect(function()
			if not isFlinging or not targetPlayer or not targetPlayer.Character then
				restoreOriginalState()
				return
			end
			
			-- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –Ω–µ —É–ª–µ—Ç–µ–ª –ª–∏ —Ü–µ–ª–µ–≤–æ–π –∏–≥—Ä–æ–∫
			if isPlayerOutOfBounds(targetPlayer) then
				statusLabel.Text = "–¶–µ–ª—å —É–ª–µ—Ç–µ–ª–∞! –í–æ–∑–≤—Ä–∞—â–∞–µ–º..."
				restoreOriginalState()
				return
			end
		end)
		
		-- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª —Ñ–ª–∏–Ω–≥–∞
		flingConnection = RunService.Heartbeat:Connect(function()
			if not isFlinging or not localHRP or not targetHRP then
				return
			end
			
			-- –ê–≥—Ä–µ—Å—Å–∏–≤–Ω—ã–π —Ñ–ª–∏–Ω–≥ —Å –±—ã—Å—Ç—Ä—ã–º –ø–æ–ª–µ—Ç–æ–º
			local currentPosition = localHRP.Position
			local targetPosition = targetHRP.Position
			
			-- –í—ã—á–∏—Å–ª—è–µ–º –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –∫ —Ü–µ–ª–∏
			local direction = (targetPosition - currentPosition).Unit
			
			-- –ú–æ—â–Ω—ã–π –∏–º–ø—É–ª—å—Å –∫ —Ü–µ–ª–∏
			localHRP.Velocity = direction * 500 + Vector3.new(0, 100, 0)
			
			-- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è –æ—á–µ–Ω—å –±–ª–∏–∑–∫–æ –∫ —Ü–µ–ª–∏ –¥–ª—è –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–≥–æ —ç—Ñ—Ñ–µ–∫—Ç–∞
			localHRP.CFrame = CFrame.new(targetPosition + direction * 2) * CFrame.Angles(0, math.random() * math.pi * 2, 0)
			
			-- –î–æ–±–∞–≤–ª—è–µ–º —Å–ª—É—á–∞–π–Ω—ã–µ –≤—Ä–∞—â–µ–Ω–∏—è –¥–ª—è —Ö–∞–æ—Ç–∏—á–Ω–æ–≥–æ –¥–≤–∏–∂–µ–Ω–∏—è
			localHRP.RotVelocity = Vector3.new(
				math.random(-50, 50),
				math.random(-50, 50),
				math.random(-50, 50)
			)
		end)
	end
	
	toggleButton.MouseButton1Click:Connect(function()
		if isFlinging then
			restoreOriginalState()
			return
		end
		
		if not currentTargetPlayer then
			statusLabel.Text = "–û—à–∏–±–∫–∞: –í—ã–±–µ—Ä–∏—Ç–µ —Ü–µ–ª—å —Å–Ω–∞—á–∞–ª–∞"
			return
		end
		
		-- –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ —Å–∞–º–æ—Ñ–ª–∏–Ω–≥
		if currentTargetPlayer == localPlayer then
			statusLabel.Text = "–û—à–∏–±–∫–∞: –ù–µ–ª—å–∑—è —Ñ–ª–∏–Ω–≥–∞—Ç—å —Å–µ–±—è!"
			targetLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
			return
		end
		
		toggleButton.Text = "STOP FLING"
		toggleButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
		statusLabel.Text = "–°—Ç–∞—Ç—É—Å: –ó–∞–ø—É—Å–∫..."
		
		-- –ó–∞–ø—É—Å–∫–∞–µ–º —Ñ–ª–∏–Ω–≥ —Å –Ω–µ–±–æ–ª—å—à–æ–π –∑–∞–¥–µ—Ä–∂–∫–æ–π
		wait(0.5)
		startFling(currentTargetPlayer)
	end)
	
	-- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è –æ—Å—Ç–∞–Ω–æ–≤–∫–∞ –ø—Ä–∏ –≤—ã—Ö–æ–¥–µ –∏–∑ –∏–≥—Ä—ã
	localPlayer.CharacterRemoving:Connect(function()
		if isFlinging then
			restoreOriginalState()
		end
	end)
	
	-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
	updateTargetInfo(nil)
end

coroutine.wrap(MainScript)()

local function DragScript()
	local script = Instance.new('LocalScript', Frame)
	script.Parent.Active = true
	script.Parent.Draggable = true
end

coroutine.wrap(DragScript)()TextLabel.BackgroundTransparency = 1.000
TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.BorderSizePixel = 0
TextLabel.Position = UDim2.new(0.112792775, 0, -0.0151660154, 0)
TextLabel.Size = UDim2.new(0, 121, 0, 26)
TextLabel.Font = Enum.Font.Sarpanch
TextLabel.Text = "Touch Fling"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextSize = 25.000

TextBox.Parent = Frame
